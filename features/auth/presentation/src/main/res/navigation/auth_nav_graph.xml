<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/signInFragment2"
    android:id="@+id/auth_nav_graph">

    <action
        android:id="@+id/action_signInFragment2_to_signUpFragment2"
        app:destination="@id/signUpFragment2" />
    <fragment
        android:id="@+id/signInFragment2"
        android:name="com.example.auth.presentation.screens.auth.sign_in.SignInFragment"
        tools:layout="@layout/fragment_sign_in"
        android:label="fragment_sign_in">
        <action
            android:id="@+id/action_signInFragment2_to_signUpFragment2"
            app:destination="@id/signUpFragment2" />

    </fragment>
    <fragment
        android:id="@+id/signUpFragment2"
        android:name="com.example.auth.presentation.screens.auth.sign_up.SignUpFragment"
        android:label="SignUpFragment" />

</navigation>